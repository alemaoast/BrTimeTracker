{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.0",
  "description": "Time Tracker",
  "permissions": [
    "activeTab",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}